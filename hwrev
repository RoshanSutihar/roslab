public static int largestGapInArray(int [] a, int n){
    
    int highDiff = 0;
    
    for (int i= 0; i<n; i++){
        
        if(Math.abs(a[i + 1] - a[i])> highDiff){
        highDiff = Math.abs(a[i + 1] - a[i]);
        }
    }
      return highDiff;
    }




